[user]
    name = aknackd
    email = adam@pyknotic.net

[alias]
    st = status
    ci = commit
    co = checkout
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    lg = log --oneline --decorate --full-index
    hist = log -15 --format=\"%Cgreen %h %Creset %ad %Cred %an %Creset %s\"
    cl = log --format=\"%Cgreen[%Cred%h%Cgreen]%Creset %s %C(bold blue)%an <%ae>%Creset\" --abbrev-commit

[diff]
    renames = copy

[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto

[core]
    compression = 9

[pack]
    window = 1000

[pretty]
    changelog = %Cgreen[%Cred%h%Cgreen]%Creset %s

[log]
	date = local

